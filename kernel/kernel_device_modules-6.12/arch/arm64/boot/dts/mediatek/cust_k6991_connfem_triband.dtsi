			"sky55208_sky55714_was",	/* [7] SKU5-2 EVB */
			"sku3__phone",			/* [8] SKU3 Phone */
			"vc536431_vc576831_was",	/* [9] SKU5-3 EVB */
			"qm42655_qm45655__phone",	/* [10] SKU1-3 Qorvo E3 Phone */
