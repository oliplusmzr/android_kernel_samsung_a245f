ifeq ($(COVERITY_BUILD),True)
DEVICE_MODULES_FOLDER := $(OUT_DIR)/$(DEVICE_MODULES_REL_DIR)
ifeq ($(wildcard $(DEVICE_MODULES_FOLDER)/*mgk*Module.symvers),)
DEVICE_MODULES_FOLDER := $(OUT_DIR)/$(DEVICE_SYMVERS_FILES_REBUILD_REL_DIR)
endif
else
DEVICE_MODULES_FOLDER := $(OUT_DIR)/$(DEVICE_MODULES_REL_DIR)
ifeq ($(wildcard $(DEVICE_MODULES_FOLDER)),)
DEVICE_MODULES_FOLDER := $(OUT_DIR)/$(subst kernel/,,$(DEVICE_MODULES_REL_DIR))
endif
endif

