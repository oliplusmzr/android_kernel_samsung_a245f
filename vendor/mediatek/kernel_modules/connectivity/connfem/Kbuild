###############################################################################
# Necessary Check
###############################################################################
ifeq ($(CFG_CONNFEM_PLAT), x86)
    KBUILD_CONNFEM_ENABLED = yes
endif

ifeq ($(KBUILD_CONNFEM_ENABLED), yes)
    KO_CODE_PATH := $(if $(filter /%,$(src)),,$(srctree)/)$(src)
    include $(KO_CODE_PATH)/Kbuild.main
endif

